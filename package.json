{
  "name": "@iocium/favicon-extractor",
  "version": "1.0.0",
  "description": "Extract favicons and app icons from HTML and manifests (Cloudflare Workers friendly)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "typedoc",
    "changelog": "auto-changelog -p"
  },
  "keywords": [
    "favicon",
    "icon",
    "apple-touch-icon",
    "android-icon",
    "web manifest",
    "web app icons",
    "favicon extractor",
    "html icon parser",
    "favicon scraper",
    "favicon fetcher",
    "manifest.json",
    "html parser",
    "meta icon",
    "seo",
    "cloudflare workers",
    "typed",
    "typescript",
    "headless browser alternative"
  ],
  "author": "Iocium",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250505.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "auto-changelog": "^2.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.4",
    "typescript": "^5.0.0"
  }
}
